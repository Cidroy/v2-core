{
	"comments": false,
	"env": {
		"main": {
			"presets": [
				["env", {
					"targets": { "node": 7 }
				}],
				"stage-0",
				"stage-2",
				"electron",
				"flow"
			]
		},
		"renderer": {
			"presets": [
				["env", {
					"modules": false
				}],
				"stage-0",
				"stage-2",
				"electron",
				"flow"
			]
		},
		"web": {
			"presets": [
				["env", {
					"modules": false
				}],
				"stage-0",
				"stage-2",
				"electron",
				"flow"
			]
		}
	},
	"plugins": ["transform-runtime","syntax-dynamic-import"]
}
