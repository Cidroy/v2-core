	"main": "./dist/electron/main.js",
	"scripts": {
		"docs:positron": "cd addons/positron/docs && gitbook serve",
		"dev:positron-webpack": "ts-node -r tsconfig-paths/register addons/positron/build/dev.ts",
		"dev:positron": "nodemon --inspect --verbose --config addons/positron/nodemon.json addons/positron/src/index.dev.ts -- --verbose",
		"build:positron": "ts-node -r tsconfig-paths/register addons/positron/build/build.ts",
		"start:positron": "npm run build:positron && node dist/positron/index.js --verbose",
		"release:positron": "ts-node -r tsconfig-paths/register build/addons/positron.ts",
		"docs:neutron": "cd addons/neutron/docs && gitbook serve",
		"dev:neutron-webpack": "ts-node -r tsconfig-paths/register addons/neutron/build/dev.ts",
		"dev:neutron": "nodemon --verbose --config addons/neutron/nodemon.json addons/neutron/src/index.dev.ts -- --verbose",
		"build:neutron": "ts-node -r tsconfig-paths/register addons/neutron/build/build.ts",
		"release:neutron": "ts-node -r tsconfig-paths/register build/addons/neutron.ts",
		"start:neutron": "npm run build:neutron && node dist/neutron/index.js",
		"dev:web": "ts-node -r tsconfig-paths/register build/web/dev.ts",
		"build:web": "ts-node -r tsconfig-paths/register build/web/build.ts",
		"start:web": "npm run build:web && http-server dist/web/",
		"docs": "cd docs && gitbook serve",
		"test": "ts-node -r tsconfig-paths/register build/electron/test.ts",
		"make-icons": "ts-node -r tsconfig-paths/register build/build.icons.ts",
		"build": "ts-node -r tsconfig-paths/register build/electron/build.ts",
		"build:linux": "cross-env BUILD_TARGET=linux ts-node -r tsconfig-paths/register build/electron/build.ts",
		"build:mas": "cross-env BUILD_TARGET=mas ts-node -r tsconfig-paths/register build/electron/build.ts",
		"build:win32": "cross-env BUILD_TARGET=win32 ts-node -r tsconfig-paths/register build/electron/build.ts",
		"dev": "ts-node -r tsconfig-paths/register build/electron/dev.ts"
	},
